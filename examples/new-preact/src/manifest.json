{
  "$schema": "https://json.schemastore.org/chrome-manifest.json",
  "manifest_version": 3,
  "version": "0.0.1",
  "name": "Extension.js - Preact New Tab Example",
  "description": "New tab page example rendered with Preact. Loads a small Preact app you can extend.",
  "icons": {
    "16": "images/preact.png",
    "48": "images/preact.png",
    "128": "images/preact.png"
  },
  "background": {
    "chromium:service_worker": "background.js",
    "firefox:scripts": ["background.js"]
  },
  "chrome_url_overrides": {
    "newtab": "newtab/index.html"
  }
}

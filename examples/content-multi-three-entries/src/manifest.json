{
  "$schema": "https://json.schemastore.org/chrome-manifest.json",
  "chromium:manifest_version": 3,
  "firefox:manifest_version": 2,
  "version": "0.0.1",
  "name": "Extension.js - JavaScript Content (Multiple Entries) Example",
  "description": "Content script example split across three entries (2 + 1 + 1). Each script injects a small element.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "chromium:service_worker": "background.js",
    "firefox:scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/script-top-left.js", "content/script-top-right.js"]
    },
    {
      "matches": ["<all_urls>"],
      "js": ["content/script-bottom-left.js"]
    },
    {
      "matches": ["<all_urls>"],
      "js": ["content/script-bottom-right.js"]
    }
  ],
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "64": "images/icon.png",
    "128": "images/icon.png"
  }
}
